{
  "name": "forecaster",
  "author": "Adam Kreitals",
  "version": "1.0.0",
  "description": "forecaster app for eTasker coding task",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -w server --exec \"babel-node server --presets es2015,stage-0\"",
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config ./webpack.production.config.babel.js --progress --profile --colors",
    "build": "npm run clean && npm run build:webpack",
    "eslint": "eslint .",
    "jscs": "jscs .",
    "flow": "flow",
    "test": "find ./server -name '*.spec.js' | xargs ./node_modules/.bin/mocha --compilers js:babel-core/register -R spec -s 1500 -t 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akreitals/coding-task.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akreitals/coding-task/issues"
  },
  "homepage": "https://github.com/akreitals/coding-task#readme",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-cli": "^6.14.0",
    "classnames": "^2.2.5",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.4.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.31.1",
    "html-webpack-plugin": "^2.22.0",
    "jscs": "^3.0.7",
    "json-stringify-safe": "^5.0.1",
    "mocha": "^3.0.2",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.11.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "winston": "^2.2.0"
  }
}
